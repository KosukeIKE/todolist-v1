<!DOCTYPE html>
<html lang="en">


<head>
    <%- include('header'); -%>
</head>

<body>
    <div class="box" id="heading">
        <h1>
            <%= titleName %>
        </h1>
    </div>

    <div class="box">
        <% for(let i=0; i < ToDoContent.length; i++){ %>
            <div class="item">
                <input type="checkbox">
                <p>
                    <%=ToDoContent[i]%>
                </p>
            </div>
            <% } %>


                <form class="item" action="/" method="POST">
                    <input type="text" name="list" placeholder=" put text!!!" autocomplete="off">
                    <button type="submit">+</button>
                </form>
    </div>

    <%- include('footer'); -%>

</body>

</html>